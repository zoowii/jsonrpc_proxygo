{
  "resolver": {
    "start": true,
    "id": "jsonrpc_proxygo_service_1",
    "name": "jsonrpc_proxygo",
    "endpoint": "consul://127.0.0.1:8500",
    "config_file_resolver": "consul://127.0.0.1:8500/v1/kv/jsonrpc_proxygo/dev/config",
    "tags": [
      "jsonrpc_proxy",
      "web",
      "dev"
    ],
    "health_checker_interval": 30
  },
  "endpoint": "127.0.0.1:5000",
  "provider": "websocket",
  "log": {
    "level": "INFO",
    "output_file": "logs/jsonrpc_proxygo.log"
  },
  "registry": {
    "start": true,
    "url": "redis://127.0.0.1:6379/1"
  },
  "plugins": {
    "upstream": {
      "upstream_endpoints": [
        {
          "url": "ws://127.0.0.1:3000",
          "weight": 1
        },
        {
          "url": "wss://127.0.0.1:4000",
          "weight": 2
        }
      ]
    },
    "http_upstream": {
      "start": false
    },
    "caches": [
      {
        "name": "dummyMethod",
        "expire_seconds": 5
      },
      {
        "name": "call",
        "paramsForCache": [
          2,
          "getSomeInfoMethod"
        ],
        "expire_seconds": 5
      }
    ],
    "before_cache_configs": [
      {
        "method": "call",
        "fetch_cache_key_from_params_count": 2
      }
    ],
    "statistic": {
      "start": true,
      "store": {
        "type": "db",
        "dbUrl": "root:123456@tcp(127.0.0.1:3306)/jsonrpc_proxygo?parseTime=true&loc=Local",
        "dumpIntervalOpened": true
      }
    },
    "disable": {
      "start": true,
      "disabled_rpc_methods": [
        "stop"
      ]
    },
    "rate_limit": {
      "start": true,
      "connection_rate": 10000,
      "rpc_rate": 1000000
    },
    "dashboard": {
      "start": true,
      "endpoint": ":5000"
    }
  }
}
